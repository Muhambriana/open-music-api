{
  "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-console": "off",
         "no-underscore-dangle": "off",
          "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always"
            }
        ]
    }
}